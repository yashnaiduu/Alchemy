{
  "name": "spirit-shelf",
  "version": "0.4.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "deploy": "ng build --base-href=/Alchemy/ && npx angular-cli-ghpages --dir=dist/browser"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.2",
  "dependencies": {
    "@angular/common": "21.0.7",
    "@angular/compiler": "21.0.7",
    "@angular/core": "21.0.7",
    "@angular/forms": "21.0.7",
    "@angular/platform-browser": "21.0.7",
    "@angular/router": "21.0.7",
    "@primeuix/themes": "^2.0.2",
    "primeng": "^21.1.1",
    "rxjs": "~7.8.0",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.3.0",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular/build": "21.0.3",
    "@angular/cli": "21.0.3",
    "@angular/compiler-cli": "21.0.7",
    "@tailwindcss/postcss": "^4.1.12",
    "angular-cli-ghpages": "^2.0.3",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}